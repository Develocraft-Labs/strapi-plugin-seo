{
  "routes": [
    {
      "method": "GET",
      "path": "/",
      "handler": "seo.index",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/find/:seoUid",
      "handler": "seo.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/seos",
      "handler": "seo.find",
      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/update/:id",
      "handler": "seo.update",
      "config": {
        "policies": ["admin::isAuthenticatedAdmin"]
      }
    },
    {
      "method": "DELETE",
      "path": "/delete/:id",
      "handler": "seo.delete",
      "config": {
        "policies": ["admin::isAuthenticatedAdmin"]
      }
    },
    {
      "method": "POST",
      "path": "/create",
      "handler": "seo.create",
      "config": {
        "policies": ["admin::isAuthenticatedAdmin"]
      }
    },
    {
      "method": "GET",
      "path": "/content-type-data/:start/:limit/:apiId/:locale",
      "handler": "seo.fetchContentTypeData",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/filter-content-type-data/:apiId/:mainField/:filter/:locale",
      "handler": "seo.filterContentTypeData",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/content-types",
      "handler": "seo.fetchContentTypes",
      "config": {
        "policies": []
      }
    }
  ]
}
